{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "types": ["react-native", "@types/node"],
    "skipLibCheck": true,
    "strict": true,
    "paths": {
      "@*": ["./*"]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "reactotron.d.ts"
  ]
}
